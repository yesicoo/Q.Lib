<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;netstandard2.1;NET6;netcoreapp3.1;</TargetFrameworks>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
    <Authors>XuQing</Authors>
    <Company>XuQing</Company>
    <PackageReleaseNotes>一款常用工具合集，日志记录，数据转换等</PackageReleaseNotes>
    <Version>1.0.8.13</Version>
    <Description>添加获取程序实际运行目录方法(针对单文件发布解压到临时目录运行优化)</Description>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.CSharp" Version="4.7.0" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
  </ItemGroup>
</Project>
